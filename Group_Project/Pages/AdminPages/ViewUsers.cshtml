@page
@model Group_Project.Pages.AdminPages.ViewUsersModel
@{
}

<h1>:: View Users ::</h1>
<br />
<h3>User : @Model.FirstName</h3>
<br />

<div>
    <table class="table">
        <thead class="table-dark">
            <tr>
                <th>UserID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email Address</th>
                <th>Role</th>
            </tr>
        </thead>

        @for (int i = 0; i < Model.Users.Count; i++)
        {
    <tr>
        <td>@Model.Users[i].UserID</td>
        <td>@Model.Users[i].FirstName</td>
        <td>@Model.Users[i].LastName</td>
        <td>@Model.Users[i].EmailAddress</td>
        <td>@Model.Users[i].Role</td>
    </tr>
        }


    </table>
</div>

<div>
    <a asp-page="/AdminPages/AdminIndex">Back to Admin Homepage</a>
</div>